{
  "compilerOptions": {
    "target": "es6",
    //"allowJs": true,
    "module": "commonjs",
    "strict": true,
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "client/*"
      ]
    },
  }
}